            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="ObScrollview" />
            </intent-filter>
        </activity>
        <activity
                android:name="com.material.ui.obscrollview.ViewPagerTabScrollViewActivity"
                android:label="@string/title_activity_viewpagertabscrollview"
                android:theme="@style/AppTheme.Toolbar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="ObScrollview" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="${UMENG_CHANNEL_VALUE}" />


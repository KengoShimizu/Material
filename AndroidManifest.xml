                <action android:name="android.intent.action.MAIN" />

                <category android:name="ObScrollview" />
            </intent-filter>
        </activity>
        <activity
                android:name="com.material.ui.obscrollview.ParallaxToolbarScrollViewActivity"
                android:label="@string/title_activity_parallaxtoolbarscrollview"

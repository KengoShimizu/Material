                <category android:name="ObScrollview" />
            </intent-filter>
        </activity>
        <activity
                android:name="com.material.ui.obscrollview.ActionBarControlWebViewActivity"
                android:label="@string/title_activity_actionbarcontrolwebview">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="ObScrollview" />
            </intent-filter>
        </activity>
        <activity
                android:name="com.material.ui.obscrollview.FlexibleSpaceToolbarScrollViewActivity"
                android:label="@string/title_activity_flexiblespacetoolbarscrollview"
